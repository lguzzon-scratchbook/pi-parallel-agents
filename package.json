{
  "name": "pi-parallel-agents",
  "version": "0.2.0",
  "description": "Dynamic parallel agent execution for pi - run multiple agents with different models without pre-defining configurations",
  "keywords": [
    "pi-package",
    "pi",
    "ai",
    "agent",
    "parallel",
    "multi-agent"
  ],
  "author": "messense",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/messense/pi-parallel-agents.git"
  },
  "type": "module",
  "main": "src/index.ts",
  "files": [
    "src",
    "README.md",
    "LICENSE"
  ],
  "peerDependencies": {
    "@mariozechner/pi-ai": "*",
    "@mariozechner/pi-coding-agent": "*",
    "@mariozechner/pi-tui": "*",
    "@sinclair/typebox": "*"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "@vitest/coverage-v8": "^4.0.18",
    "typescript": "^5.7.0",
    "vitest": "^4.0.0"
  },
  "pi": {
    "extensions": [
      "src/index.ts"
    ]
  },
  "scripts": {
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui"
  }
}
